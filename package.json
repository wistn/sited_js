{
    "name": "sited_js",
    "version": "1.4.1",
    "main": "index.js",
    "scripts": {
        "clean": "rimraf files/sited files/sited_*.txt files/logcat_stdout.txt",
        "lint": "./node_modules/.bin/eslint '{index}.js' 'lib/{[^m],m[^a]}*.js'",
        "listen_format": "onchange '{index}.js' 'lib/{[^m],m[^a]}*.js' -- prettier --write {{changed}}"
    },
    "keywords": [
        "sited",
        "plugin",
        "engine",
        "ddcat"
    ],
    "author": "wistn <wistn@qq.com> (https://github.com/wistn)",
    "license": "Apache-2.0",
    "description": "SiteD Engine for Node JavaScript version.",
    "repository": {
        "type": "git",
        "url": "https://github.com/wistn/sited_js.git"
    },
    "homepage": "https://github.com/wistn/sited_js",
    "files": [
        "/files/.gitignore",
        "/lib/*.js",
        "/index.js",
        "/package*json",
        ".gitignore",
        "CHANGELOG.md",
        "LICENSE",
        "README*"
    ],
    "dependencies": {
        "axios": "^0.21.1",
        "cheerio": "^1.0.0-rc.10",
        "iconv-lite": "^0.5.2",
        "qs": "^6.9.6"
    },
    "devDependencies": {
        "eslint": "^7.30.0",
        "eslint-config-prettier": "^7.2.0",
        "eslint-config-standard": "^16.0.3",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-plugin-promise": "^4.3.1",
        "eslint-plugin-standard": "^5.0.0",
        "prettier": "^2.3.2"
    }
}
